{
  "name": "theia-hello-world-plugin",
  "publisher": "theia",
  "keywords": [
    "theia-plugin"
  ],
  "version": "0.0.1",
  "license": "none",
  "files": [
    "src"
  ],
  "activationEvents": [
    "*"
  ],
  "devDependencies": {
    "@theia/plugin-packager": "latest",
    "clean-webpack-plugin": "^0.1.19",
    "rimraf": "2.6.2",
    "ts-loader": "^4.1.0",
    "typescript": "2.9.2",
    "typescript-formatter": "7.2.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.1.1"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "format-code": "tsfmt -r",
    "watch": "webpack-cli -w --config webpack.config.js",
    "compile": "webpack-cli --config webpack.config.js",
    "build": "yarn run format-code && yarn run compile && theia-plugin pack && mv theia_hello_world_plugin.theia ../../plugins/"
  },
  "engines": {
    "theiaPlugin": "next"
  },
  "theiaPlugin": {
    "frontend": "dist/theia-hello-world-plugin-frontend.js"
  },
  "dependencies": {
    "@theia/plugin": "file:../../packages/plugin",
    "@theia/testservice": "file:../../packages/test-service"
  }
}
