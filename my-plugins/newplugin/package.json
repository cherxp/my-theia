{
  "name": "newplugin",
  "publisher": "theia",
  "keywords": [
    "theia-plugin"
  ],
  "version": "0.0.1",
  "license": "none",
  "files": [
    "src"
  ],
  "activationEvents": [
    "*"
  ],
  "devDependencies": {
    "@theia/plugin-packager": "latest",
    "rimraf": "2.6.2",
    "typescript": "2.9.2",
    "typescript-formatter": "7.2.2"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "format-code": "tsfmt -r",
    "watch": "tsc -watch",
    "compile": "tsc",
    "build": "yarn run format-code && yarn run compile && theia-plugin pack && mv newplugin.theia ../../plugins"
  },
  "engines": {
    "theiaPlugin": "next"
  },
  "theiaPlugin": {
    "backend": "lib/newplugin-backend.js"
  },
  "dependencies": {
    "@eclipse-che/plugin": "file:../../packages/eclipse-che-theia-plugin"
  }
}
