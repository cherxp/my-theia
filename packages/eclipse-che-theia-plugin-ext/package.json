{
  "name": "@eclipse-che/theia-plugin-ext",
  "version": "0.0.1",
  "description": "Theia extension",
  "author": "Yevhen Vydolob",
  "license": "EPL-2.0",
  "keywords": [
    "theia-extension",
    "che"
  ],
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@eclipse-che/plugin": "0.0.1",
    "@theia/core": "^0.10.0",
    "@theia/plugin-ext": "^0.10.0",
    "axios": "0.18.1",
    "js-yaml": "3.12.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "ts-loader": "^4.1.0",
    "webpack": "^4.0.0",
    "webpack-cli": "2.0.12",
    "typescript-formatter": "7.2.2",
    "@types/js-yaml": "3.11.2"
  },
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "clean": "rimraf lib",
    "compile": "tsc",
    "build": "concurrently -n \"compile\" -c \"red,green,blue\" \"yarn compile\" && yarn run compileWorker",
    "compileWorker": "webpack-cli --config webpack.config.js",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/che-frontend-module",
      "backend": "lib/node/che-backend-module"
    }
  ]
}
